{
  "name": "widdershins",
  "version": "4.0.1",
  "description": "OpenAPI/ Swagger / AsyncAPI / Semoasa definition to Slate / Shins compatible markdown",
  "main": "lib/index.js",
  "bin": {
    "widdershins": "./widdershins.js",
    "testRunner": "./testRunner.js"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint": "npx eslint *.js lib/*.js test/*.js",
    "shins": "node widdershins --discovery -o ../shins/source/index.html.md defs/petstore.json",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mermade/widdershins.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "openapi3",
    "asyncapi",
    "slate",
    "shins",
    "api",
    "docs",
    "documentation",
    "semoasa"
  ],
  "author": "Mike Ralphson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mermade/widdershins/issues"
  },
  "homepage": "https://github.com/Mermade/widdershins#readme",
  "dependencies": {
    "dot": "^1.1.3",
    "highlightjs": "^9.16.2",
    "httpsnippet": "^1.22.0",
    "jgexml": "^0.4.4",
    "markdown-it": "^12.1.0",
    "markdown-it-emoji": "^2.0.0",
    "node-fetch": "^2.6.1",
    "oas-resolver": "^2.5.6",
    "oas-schema-walker": "^1.1.5",
    "openapi-sampler": "^1.1.0",
    "pinyin": "^2.9.1",
    "reftools": "^1.1.9",
    "swagger2openapi": "^7.0.8",
    "urijs": "^1.19.2",
    "yaml": "^1.10.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "mdv": "^1.3.0",
    "mocha": "^9.0.2",
    "node-readfiles": "^0.2.0",
    "nyc": "^15.0.0"
  }
}
