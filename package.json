{
  "name": "widdershins",
  "version": "4.0.1",
  "description": "OpenAPI/ Swagger / AsyncAPI / Semoasa definition to Slate / Shins compatible markdown",
  "main": "lib/index.js",
  "bin": {
    "widdershins": "./widdershins.js",
    "testRunner": "./testRunner.js"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint": "npx eslint *.js lib/*.js",
    "shins": "node widdershins --discovery -o ../shins/source/index.html.md defs/petstore.json",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mermade/widdershins.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "openapi3",
    "asyncapi",
    "slate",
    "shins",
    "api",
    "docs",
    "documentation",
    "semoasa"
  ],
  "author": "Mike Ralphson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mermade/widdershins/issues"
  },
  "homepage": "https://github.com/Mermade/widdershins#readme",
  "dependencies": {
    "dot": "^1.1.3",
    "highlightjs": "^9.16.2",
    "httpsnippet": "^1.20.0",
    "jgexml": "^0.4.3",
    "markdown-it": "^11.0.0",
    "markdown-it-emoji": "^1.4.0",
    "node-fetch": "^2.6.0",
    "oas-resolver": "^2.4.1",
    "oas-schema-walker": "^1.1.4",
    "openapi-sampler": "^1.0.0-beta.16",
    "reftools": "^1.1.3",
    "swagger2openapi": "^6.2.1",
    "urijs": "^1.19.2",
    "yaml": "^1.10.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "mdv": "^1.3.0",
    "mocha": "^8.0.1",
    "node-readfiles": "^0.2.0",
    "nyc": "^15.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
